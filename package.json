{
  "name": "vue-firebase",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "report:prep": "node -e \"require('fs').mkdirSync('cypress/report-merged',{recursive:true}); require('fs').mkdirSync('cypress/report-html',{recursive:true});\"",
    "report:merge": "npx mochawesome-merge --reportDir cypress/reports --out cypress/report-merged/mochawesome.json",
    "report:html": "npx marge cypress/report-merged/mochawesome.json --reportDir cypress/report-html --inline=false --overwrite",
    "test:e2e:html": "npm run test:e2e && npm run report:prep && npm run report:merge && npm run report:html"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "bootstrap-vue-next": "^0.40.6",
    "firebase": "^12.4.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "cypress": "^15.4.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.2"
  }
}
